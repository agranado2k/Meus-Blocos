<div data-role="content">
    <ul data-role="listview" data-theme="g" data-filter="true" >
        <% @my_blocos.each do |bloco| %>
			<li>
               <a href="/bloco/<%= bloco.id %>">
					<h3><%= bloco.name %></h3>
					<p><strong><%= bloco.venue %></strong></p>
					<p class="ui-li-aside"><strong><%= bloco.hour %></strong></p>
               </a>
            </li>
      <% end %>
    </ul>
</div><!-- /content -->